# Execution begins at address 0
    .pos 0
    irmovq stack, %rsp
    irmovq ele1, %rdi
    call sum_list
    halt

# Sample linked list
    .align 8
ele1:
    .quad 0x00a
    .quad ele2
ele2:
    .quad 0x0b0
    .quad ele3
ele3:
    .quad 0xc00
    .quad 0

sum_list:
    irmovq $0, %rax
loop:
    andq %rdi, %rdi
    je done
    mrmovq 0(%rdi), %rdx
    addq %rdx, %rax
    mrmovq 8(%rdi), %rdi
    jmp loop
done:
    ret

    .pos 0x200
stack:
