# Execution begins at address 0
    .pos 0
    irmovq stack, %rsp
    irmovq ele1, %rdi
    call rsum_list
    halt

# Sample linked list
    .align 8
ele1:
    .quad 0x00a
    .quad ele2
ele2:
    .quad 0x0b0
    .quad ele3
ele3:
    .quad 0xc00
    .quad 0

rsum_list:
    andq %rdi, %rdi
    je end
    mrmovq 0(%rdi), %rdx
    pushq %rdx
    mrmovq 8(%rdi), %rdi
    call rsum_list
    popq %rdx
    addq %rdx, %rax
    ret
end:
    irmovq $0, %rax
    ret

    .pos 0x200
stack:
